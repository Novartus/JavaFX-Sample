<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Novartus.Controller">
    <children>
        <GridPane prefHeight="400.0" prefWidth="600.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <TextArea fx:id="log" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1"/>
                <AnchorPane fx:id="pane" prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="SimpleButton1" layoutX="95.0" layoutY="89.0" mnemonicParsing="false"
                                onAction="#simpleClick" text="Simple Button 1"/>
                        <Button fx:id="SimpleButton2" layoutX="95.0" layoutY="140.0" mnemonicParsing="false"
                                text="Simple Button 2"/>
                        <Button fx:id="CancelButton" layoutX="110.0" layoutY="209.0" mnemonicParsing="false"
                                onAction="#simpleClick" text="Cancel">
                            <font>
                                <Font size="18.0"/>
                            </font>
                        </Button>
                        <Button fx:id="SubmitButton" layoutX="110.0" layoutY="268.0" mnemonicParsing="false"
                                onAction="#simpleClick" text="Submit">
                            <font>
                                <Font size="18.0"/>
                            </font>
                        </Button>
                    </children>
                </AnchorPane>
            </children>
        </GridPane>
    </children>
</AnchorPane>
